/* Sample program in C-Minus language.
 * Computes a factorial recursively.
**/
void sum(void){return 2;}
int factorial(int n) {
    if (n == 1) {
        return 1;
    } else {
        return n * factorial(n-1);
    }
}

void main(void) {
    int x;
    int f;

    x = 5; // Input an integer.
    output(factorial(x)); // Output factorial of x.
}


